{
  "manifest_version": 2,
  "name": "Long Strip for MangaDex",
  "version": "0.0.0.8",
  "web_accessible_resources" : ["script.js"],
  "icons": 
  {
    "48": "icons/48.png",
    "128": "icons/128.png" 
  }, 
  "description": "Add a long strip reading mode to MangaDex",
  "content_scripts": [
  {
    "matches": [
      "https://mangadex.org/chapter/*"
    ],
    "js": ["inject.js"]
  }]
}